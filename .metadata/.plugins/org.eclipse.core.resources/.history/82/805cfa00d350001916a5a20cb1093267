package com.JavaReview5;

public class Homework1 {
   public static void main(String[] args) {
	
       // Write a program to find the index of an array element.
  
	   int a[]= {1,2,3};
       int b=3;
       
       for(int i=0; i<a.length; ++i)
           if(a[i]==b)
        	   System.out.println(i);





	  
}

}
