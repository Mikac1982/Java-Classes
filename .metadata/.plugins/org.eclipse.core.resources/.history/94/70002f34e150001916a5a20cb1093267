package com.Practice;

public class RemoveSpV {
  public static void main(String[] args) {
	  
	  int a[]= {1,2,3,4,6,8};
      int b=3;
      int c[]=new int[a.length-1];

      
     for(int i=0,j=0;i<a.length;++i) {
          if(a[i]!=b) {
              c[j]=a[i];
              j=j+1;
              }
         
          }
     System.out.println(c);
      
	  
}
}
