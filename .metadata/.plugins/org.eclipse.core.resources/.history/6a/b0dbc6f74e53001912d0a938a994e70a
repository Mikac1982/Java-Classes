package com.class14;

public class T6 {
   public static void main(String[] args) {
	
  // Write a Java Program to find whether a String is palindrome or not?
	   
	 String str="madam";
	 String reversed="";
	 
	 for (int i=str.length()-1; i>=0; i--) {
         reversed=reversed+str.charAt(i);
	 }
	 System.out.println(reversed);
	   
	   
}
}
