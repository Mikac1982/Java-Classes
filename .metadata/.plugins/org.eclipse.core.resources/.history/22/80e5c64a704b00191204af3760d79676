package com.JavaReview5;

public class Task3 {
   public static void main(String[] args) {
	
	// ....1   
	// ...2   
	// ..3   
	// .4   
	// 5   
	   
	  
	   
	   for (int i=5; i>=1; i--)  {
		   for (int y=1; y<=i; y++)  {
			   
			   System.out.print(i);
		   }
		   System.out.println();
	   }
}
}
