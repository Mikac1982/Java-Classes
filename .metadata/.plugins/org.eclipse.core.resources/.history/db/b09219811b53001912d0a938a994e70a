package com.class14;

public class Task2222 {
  public static void main(String[] args) {
	
	  nt[] a = {1,2,3,4,5};
	  int max= a[0];
	  int max2= a[0];

	  for(int i=1; i< a.length; i++){
	  if(a[i] < max && a[i] > max2){
	  max2 = a[i];
	  System.out.println("max 2 changed:"+max2);
	  }

	  if(a[i] > max){
	  max2 = max;
	  max= a[i];
	  System.out.println("max changed:"+max);
	  }
	  }

	  System.out.println(max2);  
	  
	  
}
}
