package com.JavaReview5;

public class Homework2 {
  public static void main(String[] args) {
	
	// Write a program to find the maximum and minimum value of an array.    
	   
	int[] num= {34,6,18,66,27,41,11,79,21,45};
	
	int max=num[0];
	int min=num[0];
	
	for (int i=0; i<num.length; i++) {
		
		if (num[i]>max) {
			max=num[i];
		}
		if (num[i]<min) {
			min=num[i];
		}
	}
	System.out.println("The maximum value of an array is "+max);   
	System.out.println("The eminimum value of an array is "+min);
	
}
}
