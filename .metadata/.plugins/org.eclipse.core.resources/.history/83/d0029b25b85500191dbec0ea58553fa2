package com.class16;

public class Dog {

	//Create a Dog Class and create 3 different objects of it:
	//Husky, Bulldog, Labrador  with specific  attributes and behaviours.
	
   String breed, color;
   int age;
   
	public static void main(String[] args) {
	
	Dog dog1=new Dog();
	
	dog1.breed="Buldog";
	dog1.age=3;
	dog1.color="Braon";
	
	dog1.bark();
	dog1.play();
	dog1.sleep();
	
	Dog dog2=new Dog();
	
	dog2.breed="Labrador";
	dog2.age=2;
	dog2.color="Golden";
	
	dog2.bark();
	dog2.play();
	dog2.sleep();
	
	Dog dog3=new Dog();
	
	dog3.breed="Husky";
	dog3.age=5;
	dog3.color="White";
	
	dog3.bark();
	dog3.play();
	dog3.sleep();
		
	}
	
	void bark() {
		System.out.println("Your "+breed+" likes to bark");
	}
	void play() {
		System.out.println("My "+breed+" likes to play with kids");
	}
	void sleep() {
		System.out.println("His "+breed+" sleeps most of the time");
	}
	
}
