package com.class12;

public class Aadsdgfhjf {
    public static void main(String[] args) {
		
    	String[][] grocery= {  
			    {"bannana", "apple", "pear"},
			    {"potatos","tometoes","cabbage"},
			    {"cheese","milk","butter"}
	 };
    	
    System.out.println("-------For Each Loop--------");	
    	
    	for (String[] list:grocery) {
    		
    		for (String items:list) {
    			System.out.print(items);
    		}
    		System.out.println();
    	}
	}
}
