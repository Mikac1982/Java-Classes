package com.class11;

public class Largest {
   public static void main(String[] args) {
	
	// create an array of integers and find largest numbers  
	   
	  int[] num= {34,6,18,66,27,41,11,79,21,45};
		
	  int max=num[0];  
	   
	  for (int numbers:num) {
		  
		  if (numbers>max) {
			  System.out.println("The largest number is "+max);
		  }
	  }
	   
}
}
