package com.Java.Review6;

public class Task3 {
   public static void main(String[] args) {
	
	   int [][] ar= {{1,2,3},
               {4,5,6,9},
               {7,8}};
   
   System.out.println(ar.length);
   System.out.println(ar[0].length);
   System.out.println(ar[1].length);
   System.out.println(ar[2].length);  
	   
	   
}
}
