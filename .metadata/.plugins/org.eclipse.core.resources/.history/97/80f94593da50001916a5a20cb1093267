package com.Practice;

public class JavaArray8 {
   public static void main(String[] args) {
	
	//   Write a Java program to copy an array by iterating the array  
	   
	 int[] num= {2,4,6,8,10};
	 int[] num1=new int[num.length];
	 
	 for (int i=0; i<num.length; i++)  {
		 num[i]=num1[i]; 
	 }
	
}
}
