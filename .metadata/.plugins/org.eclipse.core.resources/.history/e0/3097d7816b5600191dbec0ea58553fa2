package com.Review7;

public class Task {

	// Create a method that has no return type
	// and have it print it "I do not have a return type"
	
	public static void main(String[] args) {
	
		Task first=new Task();
		first.noreturn();
		
	}
	void noreturn() {
		System.out.println("I do not have a return type");
	}
}
