package com.class17;

public class NoMainMethod {

	String str;
	int num;
	
	void hello() {
		System.out.println("Hello");
	}
	void bye() {
		System.out.println("Bye");
	}
	
	public static void main() {
		
		NoMainMethod obj=new NoMainMethod();   //create an object
		obj.hello();
		
	}
	
	
	
	
	
	
}
