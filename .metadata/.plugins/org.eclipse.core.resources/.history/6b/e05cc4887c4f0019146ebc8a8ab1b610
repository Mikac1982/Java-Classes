package ReplTask;

public class Task61 {
   public static void main(String[] args) {
	
   // Write a program to sum the elements in the third row.   
	   
	   int[][] nums = {
				{5,2,3,7},
				{1,5,2,2},
				{1,2,3,4}   //10
			};   
	
	
	   int row = 2;
       int total = 0;
       for (int j = 0; j < nums[row].length; j++) {
           total = total + nums[row][j];
       }
       System.out.println( total);
  
	
	   
	   
	   
   }
	  
}
