package com.class18;

import com.class18again9.*;

public class Application {

	
	public static void main(String[] args) {
         ScannerMethod obj=new ScannerMethod();	
         System.out.println(obj.sum(12, 33));		
          
	}     
	
	void example() {
		System.out.println("No return value and NO Parameters");
	}
	
	void example2(String name) {
		System.out.println("No return value but with Parameters "+name);
	}
	

	boolean example3() {
		System.out.println("With return value and NO Parameters ");
		return true;
	}
	

	String example4(String name) {
		System.out.println("With return value and with Parameters "+name);
		return name;
	}

	boolean example5(String name) { 
		System.out.println("With return value and with Parameters "+name);
		return false;
	}

	long example6(char name) { //error hapens during run time
		System.out.println("With return value and with Parameters "+name);	
	//	name=1;
		return name;
	}
	
	
	
	
}
