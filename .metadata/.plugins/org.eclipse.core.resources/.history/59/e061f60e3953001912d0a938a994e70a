package com.class14;

public class AGAIN {
   public static void main(String[] args) {
	
	// INTERVIW QUESTION!!!!!
	  	   
	// Reverse a String without using a reverse function   
	 	 
	 	// first-- using toCharArray();   
	   
	String str="Today is a Java Class";
	
	char[] array=str.toCharArray();
	
	for (char c : array) {
		System.out.println(c);
	}
	   
	   
	   
	   
	   
	   
	   
	   
}
}
