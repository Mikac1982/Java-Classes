package com.JavaReview5;

public class Task5 {
    public static void main(String[] args) {
		
    	
      	 
   for (int i=1; i<=5; i++) {
   	for (int y=1; y<=5; y++) {
   		
   		if (y!=i)  {
   		System.out.print(".");	
   		}else if (y==i) {
   			System.out.print(i);		
   		}	
   	}
   	System.out.println();
   }		
    	
    	
	}
}
