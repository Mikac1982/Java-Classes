package com.Practice;

public class Task1 {
   public static void main(String[] args) {
	

		/* I want to print pattern
		 *   *****   rows=4; columns=5;
		 *   *****
		 *   *****
		 *   *****
		 */    
	   
	for (int i=1; i<=4; i++)  {
		for (int y=1; y<=5; y++) {
			System.out.print("*");
		}
		System.out.println();
	}
}
}
