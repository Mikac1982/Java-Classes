package ReplTask;

import java.util.Scanner;

public class AA {
   public static void main(String[] args) {
	

	   int[] num=new int[5];
	   System.out.println("Input:");
	   
	   for (int i =0; i<5; i++) {
	     Scanner scan=new Scanner(System.in);
	     num[i]=scan.nextInt();
	   }
	   System.out.println();
	   System.out.println("Output:");
	   
	   for (int y=0; y<num.length; y++) {
	     System.out.println(num[y]*10);
	   }
}
}
