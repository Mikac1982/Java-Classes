package com.class11;

public class Tssssss {
    public static void main(String[] args) {
		
   // Create a 2D array that first row will contain 4 names and second row will contain grades.
   // Then you program should print name of the students that has as an A and B grade 
    	
    String[][] students= {
    		{"Una", "Ana", "Sara", "Tara"},
    		{"A", "B", "C", "D"}
    };
    
    System.out.println("Students "+students[0][0]+" and "+students[0][2]+" have grade "+students[1][0]);
    System.out.println("Students "+students[0][1]+" and "+students[0][3]+" have grade "+students[1][1]);	
	}
}
