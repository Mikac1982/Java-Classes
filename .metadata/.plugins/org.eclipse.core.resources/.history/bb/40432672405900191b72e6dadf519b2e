package com.class19;

 class Task {
	
		  protected int i = 1221;

		  void methodOfX() {
		  
		      System.out.println(i);
		  }
		}

		public class MainClass{  //another class inside Task class
			
		  public static void main(String[] args) {
		  
		      Task x = new Task();

		      System.out.println(x.i);

		      x.methodOfX();
		  }
		
		
}
