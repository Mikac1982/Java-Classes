package com.class14;

public class T8 {
   public static void main(String[] args) {
	
   //  8. Write a Java Program to print first 10 numbers of Fibonacci series.
	   
	   int a=0;
	   int b=1;
	   int c;
	   
	   System.out.println(a+" "+b);
	   
	   for (int i=2; i<=10; i++) {
		   c=a+b;
		   System.out.println(" "+c);
		   a=b;
		   b=c;
	   }
	   
	   
}
}
