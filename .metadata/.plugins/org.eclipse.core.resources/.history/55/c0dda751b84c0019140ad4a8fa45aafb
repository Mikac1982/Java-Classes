package com.JavaReview5;

public class Task3 {
   public static void main(String[] args) {
	
	// ....1   
	// ...2   
	// ..3   
	// .4   
	// 5   
	      
	   
	   
	 // second way
	   
	   for(int i =1; i<=5; i++) {
           for(int y = 1; y<=(5-i); y++) {
               System.out.print(".");
           }
           
           System.out.println(i);
           
       }
 }   

}
