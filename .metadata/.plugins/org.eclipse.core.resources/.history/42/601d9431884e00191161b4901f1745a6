package ReplTask;

public class Task61 {
   public static void main(String[] args) {
	
   // Write a program to sum the elements in the third row.   
	   
	   int[][] a = {
				{5,2,3,7},
				{1,5,2,2},
				{1,2,3,4}
			};   
	int sum=0;
	 
	for (int i=0; i<a.length; i++) {
	
		for (int y=0; y<a[i].length; y++) {
			System.out.print(a[2][y]);
			
		}
	}
	   
	   
	   
}
}
