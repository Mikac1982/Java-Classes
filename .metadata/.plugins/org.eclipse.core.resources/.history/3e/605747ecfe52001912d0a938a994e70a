package com.class14;

public class StringManipulations1 {
    public static void main(String[] args) {
		
    //toCharArray()	- converts string to array of character
    	
    String str="saturday";
    
    char[] array=str.toCharArray();
    	
    for (char c:array) {
    	System.out.println(c);
    
    }
    	
	}
}
