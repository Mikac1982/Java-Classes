package com.Practice;

public class NestedLoops {
  public static void main(String[] args) {
	
	 for (int i=0; i<=5; i++)  {
		 System.out.print(i);
	 }
	 System.out.println();
	 
	// I want to print pattern   
	// 123456789 
	// 123456789
	// 123456789
	// 123456789
	 
	 for (int i=1; i<=4; i++) {
		 
		 for (int y=1; y<=9; y++) {
			 
			 System.out.print(y);
		 }
		 System.out.println(); 
	 }
	 
	 
}
}
