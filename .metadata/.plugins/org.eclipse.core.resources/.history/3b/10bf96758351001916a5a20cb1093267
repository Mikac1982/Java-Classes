package com.Java.Review6;

public class Aaaaa {
   public static void main(String[] args) {
	
   // Print the average of values in column 5.   
		   //  2 6 9 3 7
		   //  1 7 9 4 8
		   //  0 4 6 2 5
		   //  0 1 2 3
		    	 
    int[][] ar = { { 2, 6, 9, 3, 7 }, { 1, 7, 9, 4, 8 }, { 0, 4, 6, 2, 5 }, { 0, 1, 2, 3 } };	
	
    double sum4=0;
    
	for (int i=0; i<ar.length; i++ ) {
		
		sum4+=ar[i][3];
	}
    System.out.println(sum4);
    
    
}
}
