package com.JavaReview5;

public class Task3 {
   public static void main(String[] args) {
	
	// ....1   
	// ...2   
	// ..3   
	// .4   
	// 5   
	   
	  
	   
	   for (int i=1; i<=5; i++)  {
		   for (int y=5; y<=i; y--)  {
			   
			   System.out.print(y);
		   }
		   System.out.println();
	   }
}
}
