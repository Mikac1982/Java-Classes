package com.class11;

public class Sum {
  public static void main(String[] args) {
	
	// Create an array on integers and calculate the sum of all elements in an array. 
	  
	int[] num= {2,4,6,8,10,12,14,16};
	int sum=0;
	
	for (int i=0; i<num.length; i++) {
		sum=sum+i;
	}
	 System.out.println(sum); 
	  
	  
	  
	  
}
}
