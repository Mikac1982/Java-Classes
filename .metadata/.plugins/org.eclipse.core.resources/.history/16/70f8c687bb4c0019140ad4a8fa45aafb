package com.JavaReview5;

public class Task4Ag {
   public static void main(String[] args) {
	
	   //   ....1
       //   ...2.
       //   ..3..
       //   .4...
       //   5....    
	   
	for (int i=1; i<=5; i++)  {
		for (int y=5; y>=1; y--) {
			
			if (y!=i) {
			    System.out.print(".");
			}else if (y==i) {
				System.out.print(i);
			}
		    }
			System.out.println();
		}     
    }
}
