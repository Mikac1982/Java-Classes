package com.Practice;

public class Task1 {
  public static void main(String[] args) {
	// 24 hours 60 minutes
	  
	for (int i=0; i<24; i++)   {
		for (int y=0; y<60; y++) {
			System.out.println(i+":"+y);
		
		     if (y<10) {
			     System.out.print("0:");
		     }else {
		    	 System.out.print();
		     }
	}
	}  
	  
}
}
