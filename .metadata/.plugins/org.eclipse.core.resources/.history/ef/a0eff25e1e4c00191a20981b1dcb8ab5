package com.Practice;

public class T2 {
     public static void main(String[] args) {
		
 //       *        row=5; col=9; 
 //      ***
 //     *****
 //    *******
 //   *********	 
    
   for (int i=5; i>=1; i--) {
	   for (int y=9; y>=1; y--) {
		   
		   if (y==i) {
			   System.out.print("*");
		   }else {
			   System.out.print(" ");
		   }
		   System.out.println();
	   }
   }	  
    	 
    	 
    	 
    	 
    	 
	}
}
