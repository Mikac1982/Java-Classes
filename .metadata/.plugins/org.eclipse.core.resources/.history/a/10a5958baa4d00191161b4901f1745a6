package com.class11;

public class Again {
  public static void main(String[] args) {
	
	// create an array of integers and find largest number and smallest	  
	 
	int[] arr= {23,5,66,84,31,26,885,243,11,59};
	int largest=arr[0];
	int smallest=arr[0];
	
	for (int i=0; i<arr.length; i++)  {
		
		if (arr[i]>largest) {
			largest=arr[i];
		}else if (arr[i]<smallest) {
			smallest=arr[i];
		}
	}
	  
	  
	  
	  
	  
	  
	  
	  
	  
	  
	  
	  
	  
}
}
