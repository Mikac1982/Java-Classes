package com.class18_2;

public class ScannerMethod {

	public static void main(String[] args) {
		
		ScannerMethod obj=new ScannerMethod();	
		obj.sum(14, 16);
		
	}
	
	void sum(int a,int b) {
		
		System.out.println(a+b);
		
	}
	
	void sub() {
		int a=10;
	    int b=12;
		System.out.println(a+b);
		
	}
}
