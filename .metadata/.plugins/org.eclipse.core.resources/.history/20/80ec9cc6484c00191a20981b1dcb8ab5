package com.Practice;

public class T2 {
	public static void main(String[] args) {

		// * row=5; col=9;
		// ***
		// *****
		// *******
		// *********

		for (int i = 1; i <= 5; i++) {
			for (int y = 1; y <= (5 - i); y++) {
				System.out.print(" ");
			}
			for (int y = 1; y <= i; y++) {
				System.out.print("*");
			}
		System.out.println();
		}
	}
}
