package com.Practice;

public class T2 {
     public static void main(String[] args) {
		
 //       *        row=5; col=9; 
 //      ***
 //     *****
 //    *******
 //   *********	 
    
   for (int i=5; i>=1; i--) {
	   for (int y=1; y<=i-1; y++) {
		   System.out.print(" ");
	   }
	   System.out.print("*");
   }
   System.out.println();
	 
  }
}
