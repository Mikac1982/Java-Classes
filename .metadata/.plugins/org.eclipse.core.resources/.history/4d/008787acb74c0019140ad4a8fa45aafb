package com.JavaReview5;

public class Task3Ag {
   public static void main(String[] args) {
	
	// ....1   
	// ...2   
	// ..3   
	// .4   
	// 5   
		         
	for (int i=1; i<=5; i++)   {
		for (int y=1; y<=5-i; y++)  {
			System.out.print(".");
		}
		for (int y=5; y==i; y--) {
			System.out.print(i);
		}
		System.out.println();
	}
	   
	   
}
}
