package com.class8;

public class Task11 {
  public static void main(String[] args) {
	  
	  // print numbers from 1 to 100
	  
	  for (int i=1; i<=100; i++) {
		  System.out.print(i+" ");
	}
	  System.out.println();
	  
	  // print numbers from 100 to 1
	  
	  for (int i=100; i>=1; i--) {
		  System.out.print(i+" ");
	  }
	
}
}
