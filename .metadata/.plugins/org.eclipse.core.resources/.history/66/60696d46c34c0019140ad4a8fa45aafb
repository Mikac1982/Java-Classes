package com.JavaReview5;

public class Square {
   public static void main(String[] args) {
	
	    // I want to print pattern
		// ******   rows=5; columns=6
		// *    *
		// *    *
	    // *    *
		// ******  
	   
	 for (int i=1; i<=5; i++)  {
		 for (int y=1; y<=6; y++) {
			 
			 if (i==1 || i==5 || y==1 || y==6) {
			   System.out.print("*");
	        }else 
	           System.out.print(" ");
		    }
		 System.out.println();
	 }   
}
}
