package Quizz;

public class Q1 {
  public static void main(String[] args) {
	  int i[] = {12, 15, 16, 17, 19};

	  int a[][] = {{1, 3, 4}, {2, 3}, {7, 6, 5, 4, 3, 2, 1}, {9}, {8}};
	  int sum = 0;
	  for(int y = 0; y < a.length; y++)
	  {
	  for(int j = 0; j < a[0].length; j++)
	  {
	  sum += a[y][j];
	  }
	  }
	  System.out.println("sum = " + sum);
	  }
	  
}  

