package com.JavaReview5;

public class Task2Ag {
   public static void main(String[] args) {
	
	   // ....1   rows=5 columns=5
	   // ...22
	   // ..333
	   // .4444	
	   // 55555	   
	   
	for (int i=1; i<=5; i++)   {
		for (int y=1; y<=5-i; y++) {
			System.out.print(".");
		}
		System.out.print(i);
	}
	System.out.println(); 
}
}
