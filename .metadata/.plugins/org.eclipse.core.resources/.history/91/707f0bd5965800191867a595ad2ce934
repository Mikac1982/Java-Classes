package com.class18again9;

import com.class18.Calculator;

public class Aplications {

	
	public static void main(String[] args) {
		
		Calculator obj=new Calculator();
		
		System.out.println(obj.sum(3, 4));
		System.out.println(obj.sum(4, 6));
		System.out.println(obj.div(20, 4));
		System.out.println(obj.mult(3, 6));
	}
}
