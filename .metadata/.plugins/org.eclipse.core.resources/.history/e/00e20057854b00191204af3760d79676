package com.class9;

public class NestedLoops {
   public static void main(String[] args) {
	
	   for (int i=0; i<3; i++) {        //i --> counter variable
		  System.out.println("------I am outer loop--------");
		  
	       for (int y=0; y<3; y++) {     //
	    	  System.out.println("I am inner loop");
	      }
	       
	     }
	    
   }
}
