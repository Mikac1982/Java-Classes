package com.Java.Review6;

public class Again {
  public static void main(String[] args) {
	
	// Write a program to print a sum of the second row
		  
		int[][]  arr= {
				{1,2,3},
				{4,5,6},
				{7,8,9}
		};  
	  
	 int sum=0;
	 
	 for (int i=0; i<arr[1].length; i++)  {
		 sum+=arr[1][i];
	 }
	 System.out.println(sum);  // 15
	  
	  
}
}
