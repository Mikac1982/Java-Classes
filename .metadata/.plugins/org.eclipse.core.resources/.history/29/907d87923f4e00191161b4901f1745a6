package com.class12;

public class Recap {
    public static void main(String[] args) {
		
    int[][] arr= {
    		{1,2,3},
    		{10,20,30},
    		{100,200,300}
    };
	
	for (int i=0; i<arr.length; i++) {  //how many arrays inside my array -->3
		
		for (int y=0; y<arr[i].length; y++)  {
			System.out.print(arr[i][y]);
		}
		System.out.println();
	}
	
	
	
	
	
	
    }	
}
